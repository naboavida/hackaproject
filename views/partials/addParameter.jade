div(style='width:400px;margin-left:auto;margin-right:auto;margin-top:100px;')
	h1 Add a Parameter
	div(ng-controller="ParameterCtrl")
		li(ng-repeat='parameter in parameters')
			p {{parameter.title}} {{parameter.value}}
		//- form(action='/addProject', method='post')
		div
			p: input(placeholder='Title', name='title', ng-model='form.title')
			p: input(placeholder='Value', name='value', ng-model='form.value')
			p: button(ng-click='submitNewParameter()') Adicionar
	p: a(href='/indicator/{{pid}}/{{iid}}') Back
	p: a(href='/') Home